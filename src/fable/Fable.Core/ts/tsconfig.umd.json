{
    "compilerOptions": {
        "target": "es5",
        "lib": [ "es6", "dom" ],
        "module": "umd",
        "allowJs": true,
        "noEmitOnError": true,
        "noFallthroughCasesInSwitch": true,
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "removeComments": true,
        //"noUnusedLocals": true,
        //"noUnusedParameters": true,
        //"strictNullChecks": true,
        "declaration": false,
        "outDir": "../../../../build/fable-core/umd"
    }
}